(define-extern aphid type)
(defun mod-override-path ((obj nav-enemy))
  (case (-> obj type)
    ((aphid)
      (set-vector-meters! (-> obj path cverts 0) 441.7 -44.0 -288.1)
      (set-vector-meters! (-> obj path cverts 1) 426.6 -44.0 -288.1)
      (set-vector-meters! (-> obj path cverts 2) 426.6 -44.0 -260.1)
      (set-vector-meters! (-> obj path cverts 3) 441.7 -44.0 -260.1)
      (vector-copy! (-> obj nav destination-pos) (-> *target* root trans))
      )
    (else
      (when #f
        (format 0 "Unhandled mod-override-path type ~A~%" (-> obj type))
        )
      )
    )
  (none)
  )

(defun mod-run-each-frame ()
  ;; Geyser

  ;; Sandover

  ;; FJ outside
  (when (process-by-ename "ecovent-193")  ;; above fisherman where orig mirrors is
    (let ((obj (the ecovent (process-by-ename "ecovent-193"))))
      (set-vector-meters! (-> obj root trans) 261.9 37.2 -258.0)
      (set-vector-meters! (-> obj root-override trans) 261.9 37.2 -258.0)
      (set-vector-meters! (-> obj root-override root-prim prim-core world-sphere) 261.9 37.2 -258.0)
      )
    )
  (when (process-by-ename "eco-110")  ;; on orig orb cache
    (let ((obj (the eco-blue (process-by-ename "eco-110"))))
      ;; only move if not being suck-sucked
      (when (not (or (name= (-> obj state name) 'pickup) (name= (-> obj state name) 'notice-blue)))
        (set-vector-meters! (-> obj base) 318.1 3.5 -326.1)
        (set-vector-meters! (-> obj root trans) 318.1 3.5 -326.1)
        (set-vector-meters! (-> obj root-override trans) 318.1 3.5 -326.1)
        (set-vector-meters! (-> obj root-override root-prim prim-core world-sphere) 318.1 3.5 -326.1)
        )
      )
    )
  (when (process-by-ename "eco-111")  ;; lower river
    (let ((obj (the eco-blue (process-by-ename "eco-111"))))
      ;; only move if not being suck-sucked
      (when (not (or (name= (-> obj state name) 'pickup) (name= (-> obj state name) 'notice-blue)))
        (set-vector-meters! (-> obj base) 407.6 1.5 -311.1)
        (set-vector-meters! (-> obj root trans) 407.6 1.5 -311.1)
