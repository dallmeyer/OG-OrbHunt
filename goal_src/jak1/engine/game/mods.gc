;;-*-Lisp-*-
(in-package goal)

(defun move-actor ((actor string) (x float) (y float) (z float))
  (when (process-by-ename actor)
    (set-vector! (-> (the process-drawable (process-by-ename actor)) root trans) (meters x) (meters y) (meters z) 1.0)

    (cond
      ((type-type? (-> (process-by-ename actor) type) fuel-cell)
       (set! (-> (the fuel-cell (process-by-ename actor)) base y) (meters y))
       )
      ((type-type? (-> (process-by-ename actor) type) money)
       (set! (-> (the money (process-by-ename actor)) base x) (meters x))
       (set! (-> (the money (process-by-ename actor)) base y) (meters y))
       (set! (-> (the money (process-by-ename actor)) base z) (meters z))
       )
      ((type-type? (-> (process-by-ename actor) type) crate)
       (set! (-> (the crate (process-by-ename actor)) base y) (meters y))
       )
      )
    )
  )

